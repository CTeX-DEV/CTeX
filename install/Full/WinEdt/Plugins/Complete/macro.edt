// build DDE connection for command completion
// WinEdt macros
GlobalMark;
CMD("Select Word");
GetSel(0,9);                   // Get Selected Text in Register %!9
DDEOpen("%b\Plugins\complete\complete.exe", "Complete", "CommandServer");
DDEExe("Macro,%!9");
DDEClose;
LetReg(9,"");                  // Release Memory for Register %!9 
End;
